<?xml version="1.0"?>
<cd:Record format="binary" name="MIP_NL__1P_ADSR_geolocation" last-modified="2010-08-26" xmlns:cd="http://www.stcorp.nl/coda/definition/2008/07">
  <cd:Field name="dsr_time">
    <cd:Time timeformat="binary_envisat_datetime">
      <cd:Description>ZPD time of the first sweep in the scan</cd:Description>
    </cd:Time>
  </cd:Field>
  <cd:Field name="attach_flag">
    <cd:Integer>
      <cd:Description>Attachment Flag(set to 1 if all MDSRs associated with this ADSR are blank or missing. Set to zero otherwise.)</cd:Description>
      <cd:BitSize>8</cd:BitSize>
      <cd:NativeType>uint8</cd:NativeType>
    </cd:Integer>
  </cd:Field>
  <cd:Field name="time_mid">
    <cd:Time timeformat="binary_envisat_datetime">
      <cd:Description>ZPD time of the sweep closest in time to the center of the scan</cd:Description>
    </cd:Time>
  </cd:Field>
  <cd:Field name="time_last">
    <cd:Time timeformat="binary_envisat_datetime">
      <cd:Description>ZPD time of the last sweep in the scan</cd:Description>
    </cd:Time>
  </cd:Field>
  <cd:Field name="loc_first">
    <cd:Record>
      <cd:Description>WGS84 latitude and longitude of first sweep in the scan</cd:Description>
      <cd:Field name="latitude">
        <cd:Integer>
          <cd:Unit>1e-6 degrees_north</cd:Unit>
          <cd:Conversion numerator="1.000000" denominator="1000000.000000">
            <cd:Unit>degrees_north</cd:Unit>
          </cd:Conversion>
          <cd:BitSize>32</cd:BitSize>
          <cd:NativeType>int32</cd:NativeType>
        </cd:Integer>
      </cd:Field>
      <cd:Field name="longitude">
        <cd:Integer>
          <cd:Unit>1e-6 degrees_east</cd:Unit>
          <cd:Conversion numerator="1.000000" denominator="1000000.000000">
            <cd:Unit>degrees_east</cd:Unit>
          </cd:Conversion>
          <cd:BitSize>32</cd:BitSize>
          <cd:NativeType>int32</cd:NativeType>
        </cd:Integer>
      </cd:Field>
    </cd:Record>
  </cd:Field>
  <cd:Field name="loc_mid">
    <cd:Record>
      <cd:Description>WGS84 latitude and longitude of the sweep closest in time to the center of the scan</cd:Description>
      <cd:Field name="latitude">
        <cd:Integer>
          <cd:Unit>1e-6 degrees_north</cd:Unit>
          <cd:Conversion numerator="1.000000" denominator="1000000.000000">
            <cd:Unit>degrees_north</cd:Unit>
          </cd:Conversion>
          <cd:BitSize>32</cd:BitSize>
          <cd:NativeType>int32</cd:NativeType>
        </cd:Integer>
      </cd:Field>
      <cd:Field name="longitude">
        <cd:Integer>
          <cd:Unit>1e-6 degrees_east</cd:Unit>
          <cd:Conversion numerator="1.000000" denominator="1000000.000000">
            <cd:Unit>degrees_east</cd:Unit>
          </cd:Conversion>
          <cd:BitSize>32</cd:BitSize>
          <cd:NativeType>int32</cd:NativeType>
        </cd:Integer>
      </cd:Field>
    </cd:Record>
  </cd:Field>
  <cd:Field name="loc_last">
    <cd:Record>
      <cd:Description>WGS84 latitude and longitude of last sweep in the scan</cd:Description>
      <cd:Field name="latitude">
        <cd:Integer>
          <cd:Unit>1e-6 degrees_north</cd:Unit>
          <cd:Conversion numerator="1.000000" denominator="1000000.000000">
            <cd:Unit>degrees_north</cd:Unit>
          </cd:Conversion>
          <cd:BitSize>32</cd:BitSize>
          <cd:NativeType>int32</cd:NativeType>
        </cd:Integer>
      </cd:Field>
      <cd:Field name="longitude">
        <cd:Integer>
          <cd:Unit>1e-6 degrees_east</cd:Unit>
          <cd:Conversion numerator="1.000000" denominator="1000000.000000">
            <cd:Unit>degrees_east</cd:Unit>
          </cd:Conversion>
          <cd:BitSize>32</cd:BitSize>
          <cd:NativeType>int32</cd:NativeType>
        </cd:Integer>
      </cd:Field>
    </cd:Record>
  </cd:Field>
  <cd:Field name="spare_1">
    <cd:Raw>
      <cd:Description>Spare</cd:Description>
      <cd:BitSize>64</cd:BitSize>
    </cd:Raw>
    <cd:Hidden/>
  </cd:Field>
</cd:Record>
