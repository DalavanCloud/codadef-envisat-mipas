      <cd:Description>Altitude grid for VMR base profiles</cd:Description>
      <cd:Dimension><![CDATA[int(/dataset_structure_ads[$vmr1_structure_index[index(:/..)]]/num_base_vmr_pts[0])]]></cd:Dimension>
      <cd:Float>
        <cd:Unit>km</cd:Unit>
        <cd:BitSize>32</cd:BitSize>
        <cd:NativeType>float</cd:NativeType>
      </cd:Float>
    </cd:Array>
  </cd:Field>
  <cd:Field name="base_vmr">
    <cd:Array>
      <cd:Description>VMR profile consisting of retrieved VMR and assumed values above and below (= base profile)</cd:Description>
      <cd:Dimension><![CDATA[int(/dataset_structure_ads[$vmr1_structure_index[index(:/..)]]/num_base_vmr_pts[0])]]></cd:Dimension>
      <cd:Float>
        <cd:Unit>ppmv</cd:Unit>
        <cd:BitSize>32</cd:BitSize>
        <cd:NativeType>float</cd:NativeType>
      </cd:Float>
    </cd:Array>
  </cd:Field>
  <cd:Field name="avg_kernel">
    <cd:Array>
      <cd:Description>Averaging kernel matrix</cd:Description>
      <cd:Dimension><![CDATA[int(/dataset_structure_ads[$vmr1_structure_index[index(:/..)]]/num_vmr_pts[0]) * (int(/dataset_structure_ads[$vmr1_structure_index[index(:/..)]]/num_vmr_pts[0]) + 1) / 2]]></cd:Dimension>
      <cd:Float>
        <cd:BitSize>32</cd:BitSize>
        <cd:NativeType>float</cd:NativeType>
      </cd:Float>
    </cd:Array>
  </cd:Field>
  <cd:Field name="cond_param">
    <cd:Float>
      <cd:Description>conditioning parameter</cd:Description>
      <cd:BitSize>32</cd:BitSize>
      <cd:NativeType>float</cd:NativeType>
    </cd:Float>
  </cd:Field>
</cd:Record>
