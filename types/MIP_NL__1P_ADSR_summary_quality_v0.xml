      <cd:Integer>
        <cd:BitSize>16</cd:BitSize>
        <cd:NativeType>uint16</cd:NativeType>
      </cd:Integer>
    </cd:Array>
  </cd:Field>
  <cd:Field name="num_opd_shift">
    <cd:Array>
      <cd:Description>Number of sweeps for which the OPD shift in band B differs from band AB. Sequence is: forward direction, reverse direction</cd:Description>
      <cd:Dimension>2</cd:Dimension>
      <cd:Integer>
        <cd:BitSize>16</cd:BitSize>
        <cd:NativeType>uint16</cd:NativeType>
      </cd:Integer>
    </cd:Array>
  </cd:Field>
  <cd:Field name="num_sweeps_flux_oor">
    <cd:Integer>
      <cd:Description>Number of sweeps for which the flux is out of range for one or all detectors</cd:Description>
      <cd:BitSize>16</cd:BitSize>
      <cd:NativeType>uint16</cd:NativeType>
    </cd:Integer>
  </cd:Field>
  <cd:Field name="spare_2">
    <cd:Raw>
      <cd:Description>Spare</cd:Description>
      <cd:BitSize>176</cd:BitSize>
    </cd:Raw>
    <cd:Hidden/>
  </cd:Field>
